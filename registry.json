{
  "extensions": [
    {
      "id": "jongio.azd.rest",
      "namespace": "rest",
      "displayName": "REST API Extension",
      "description": "Execute REST API calls with Azure authentication and scope detection.",
      "versions": [
        {
          "version": "0.1.0",
          "capabilities": [
            "custom-commands"
          ],
          "usage": "azd rest \u003cmethod\u003e \u003curl\u003e [options]",
          "examples": [
            {
              "name": "get",
              "description": "Execute GET request",
              "usage": "azd rest get https://management.azure.com/subscriptions?api-version=2020-01-01"
            },
            {
              "name": "version",
              "description": "Display extension version",
              "usage": "azd rest version"
            }
          ]
        },
        {
          "version": "0.1.1",
          "capabilities": [
            "custom-commands",
            "lifecycle-events",
            "mcp-server",
            "metadata"
          ],
          "usage": "azd rest \u003cmethod\u003e \u003curl\u003e [options]",
          "examples": [
            {
              "name": "get",
              "description": "Execute a GET request against Azure Resource Manager",
              "usage": "azd rest get https://management.azure.com/subscriptions?api-version=2022-12-01"
            },
            {
              "name": "post",
              "description": "Execute a POST request with a JSON body",
              "usage": "azd rest post \u003curl\u003e --body '{\"key\": \"value\"}'"
            },
            {
              "name": "version",
              "description": "Display extension version",
              "usage": "azd rest version"
            }
          ],
